{% extends 'base.html.twig' %}


{% block title %}{{ parent() }} - Map{% endblock %}



{% block body %}
    <div class="container my-5">
        <h1>{{ pictureGpsPoints|length }} pictures</h1>
        <div id="map"
             class="rounded"
             data-pictures="{{ pictureGpsPoints|serialize }}"
        >

        </div>
    </div>
{% endblock %}

{% block stylesheets %}
    {{ encore_entry_link_tags('map') }}
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('map-multiple-js') }}
{% endblock %}