<a href="{{ path('CATALOG', {'id': catalog.id}) }}" class="catalog-card card shadow-sm h-100 d-block" data-bs-toggle="tooltip"
   data-bs-placement="top" title="{{ catalog.name }}">
    <img src="{{ catalog.imageName|imagine_filter('catalog_card_listing') }}" class="card-img-top"
         alt="{{ catalog.name }}">
    <div class="card-body p-1">
        <div class="d-flex flex-column justify-content-between h-100">
            <p class="card-title text-truncate mb-0">
                <small class="">
                    {{ catalog.name }}
                </small>
            </p>
            <p class="card-title">

                <small>
                    [{{ catalog.pictures|length }}]
                </small>
            </p>
        </div>
    </div>
</a>