{% extends 'base-admin.html.twig' %}

{% block title %}Place{% endblock %}

{% block body %}
    <h1>Place</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ place.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ place.name }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ place.location }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ place.lat }}</td>
            </tr>
            <tr>
                <th>Lng</th>
                <td>{{ place.lng }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('ADMIN_CATALOG_PLACE_INDEX') }}">back to list</a>

    <a href="{{ path('ADMIN_CATALOG_PLACE_EDIT', {'id': place.id}) }}">edit</a>

    {{ include('catalog/place/_delete_form.html.twig') }}
{% endblock %}
