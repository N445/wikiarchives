{% extends 'base-admin.html.twig' %}

{% block title %}Modification d'une photo {{ picture.name }} - {{ parent() }}{% endblock %}

{% block body %}
<div class="container">
    <h1>Modification d'une photo {{ picture.name }}</h1>

    <a href="{{ path('ADMIN_CATALOG_PICTURE_INDEX') }}" class="btn btn-primary mb-3">
        Revenir au listing
    </a>

    {{ include('catalog/picture/_delete_form.html.twig') }}

    {{ include('catalog/picture/_form.html.twig', {'button_label': 'Modifier'}) }}
</div>
{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('wysiwyg-js') }}
{% endblock %}