<form method="post" action="{{ path('ADMIN_CATALOG_PLACE_DELETE', {'id': place.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir {{ 'actions.delete'|trans([],'admin') }} cet élément ?');">
